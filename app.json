{
	"expo": {
		"name": "HopMate",
		"slug": "HopMate",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/logo.png",
		"scheme": "com.hopmate",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/logo.png",
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true,
			"package": "com.vegetaue.HopMate",
			"permissions": [
				"android.permission.ACCESS_COARSE_LOCATION",
				"android.permission.ACCESS_FINE_LOCATION"
			]
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/logo.png"
		},
		"plugins": [
			"@maplibre/maplibre-react-native",
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/logo.jpg",
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "1c5a9aaa-4edf-42fe-97ac-98c80305b7f2"
			}
		},
		"owner": "vh-x"
	}
}
